<div id="navbar" class="navbar navbar-default">
		<script type="text/javascript">
		try{ace.settings.check('navbar' , 'fixed')}catch(e){}
		</script>
		<div class="navbar-container" id="navbar-container">

			<!--Toggle Sidebar Button (with three lines)-->
			<button type="button" class="navbar-toggle menu-toggler pull-left" id="menu-toggler">
				<span class="sr-only">Toggle sidebar</span> <!--Invisible-->
				<span class="icon-bar"></span><!--Eachline in the button-->
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</button>

			<!--Logo/Name button, next to toggle sidebar button-->
			<div class="navbar-header pull-left">
				<a href="<% root_path %>" class="navbar-brand"><!--Clickable-->
					<small>
						<i class="fa fa-cloud"></i><!--logo-->
						TechStorm
					</small>
				</a>
			</div>

			<!-- Buttons to the right-->
			<div class="navbar-buttons navbar-header pull-right" role="navigation">
				<ul class="nav ace-nav">

					<%= render 'layouts/navigation_links' %>

				</ul>
			</div>
		</div>
	</div>