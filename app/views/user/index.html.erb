<h1>Users</h1>
<% @users.each do |user| %>
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="../assets/placeholder.png">
      <div class="caption">
        <h3><%=user.first_name%> <%=user.last_name %></h3>
        <%= button_to "VIEW", {:action => "show", :class => "button", :method => :get, :id => user.id}%>
      </div>
    </div>
  </div>
<% end %>